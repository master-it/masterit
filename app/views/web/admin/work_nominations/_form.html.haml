= simple_form_for @work_nomination, :url => url, :html => { :class => 'default_class' } do |f|
  = f.input :state_event, as: :state_event, wrapper: :state
  = f.input :name
  .form-actions
    = f.button :submit, :class => 'btn btn-primary'
    = link_to t('.cancel'), admin_work_nominations_path, :class => 'btn'
